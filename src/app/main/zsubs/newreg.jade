#newreg.modal.fade(tabindex="-1",role="dialog",aria-labelledby="mvmodallabel",aria-hidden="true")
  .modal-dialog.modal-lg
    .modal-content
      .modal-header
        button.close(data-dismiss="modal")
          span(aria-hidden="true") &times;
          span.sr-only Close
